AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,iDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAVA;AAWA,IAAA,UAXA,sBAWA,IAXA,EAWA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAfA;AAgBA,IAAA,WAhBA,uBAgBA,KAhBA,EAgBA;AACA,WAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAlBA;AAmBA,IAAA,SAnBA,qBAmBA,IAnBA,EAmBA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KArBA;AAuBA,IAAA,SAvBA,qBAuBA,IAvBA,EAuBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAMA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAPA,kDAQA,IARA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAnCA;AAoCA,IAAA,QApCA,sBAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA;AA5CA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"post-gallery\">\r\n    <div class=\"post\" v-for=\"item in items\" :key=\"item.id\">\r\n      <img :src=\"item.path\" />\r\n\r\n      <div class=\"postInfo\">\r\n        <h2>{{ item.title }}</h2>\r\n        <p>{{ item.textarea }}</p>\r\n      </div>\r\n      <div class=\"edit\">\r\n        <div class=\"form\">\r\n          <button @click=\"deleteItem(item)\">Delete</button>\r\n          <button @click=\"editItem1(item)\">Edit</button>\r\n        </div>\r\n        <div class=\"upload\" v-if=\"findItem\">\r\n          <input v-model=\"findItem.title\" />\r\n          <br />\r\n          <br />\r\n          <input class=\"textareaInput\" v-model=\"findItem.textarea\" />\r\n          <input type=\"file\" name=\"photo\" @change=\"fileChanged\" />\r\n          <div class=\"actions\" v-if=\"findItem\">\r\n            <button @click=\"editItem2(findItem)\">post</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\n\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      title: \"\",\r\n      file: null,\r\n      addItem: null,\r\n      items: [],\r\n      findTitle: \"\",\r\n      findItem: null,\r\n      textarea: \"\",\r\n      findTextarea: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    suggestions() {\r\n      let items = this.items.filter((item) =>\r\n        item.title.toLowerCase().startsWith(this.findTitle.toLowerCase())\r\n      );\r\n      return items.sort((a, b) => a.title > b.title);\r\n    },\r\n  },\r\n  created() {\r\n    this.getItems();\r\n  },\r\n  methods: {\r\n    async deleteItem(item) {\r\n      try {\r\n        await axios.delete(\"/api/items/\" + item._id);\r\n        this.findItem = null;\r\n        this.getItems();\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    selectItem(item) {\r\n      this.findTitle = \"\";\r\n      this.findTextarea = \"\";\r\n      this.findItem = item;\r\n    },\r\n    fileChanged(event) {\r\n      this.file = event.target.files[0];\r\n    },\r\n    editItem1(item) {\r\n      this.findItem = item;\r\n    },\r\n\r\n    async editItem2(item) {\r\n      try {\r\n        await axios.put(\"/api/items/\" + item._id, {\r\n          title: this.findItem.title,\r\n          textarea: this.findItem.textarea,\r\n        });\r\n        this.findItem = null;\r\n        this.getItems();\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async getItems() {\r\n      try {\r\n        let response = await axios.get(\"/api/items\");\r\n        this.items = response.data;\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.post-gallery {\r\n  display: flex;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  justify-content: start;\r\n  border-color: red;\r\n  border-width: 10px;\r\n  border-style: solid;\r\n  width: 70%;\r\n  margin-top: 10%;\r\n}\r\n\r\n.post {\r\n  display: flex;\r\n  width: 50%;\r\n  margin: 40px;\r\n  align-items: center;\r\n  border-color: rgb(25, 0, 255) d;\r\n  border-width: 10px;\r\n  border-style: solid;\r\n  flex-direction: column;\r\n}\r\n\r\n.post img {\r\n  width: 70%;\r\n  border-color: rgb(25, 0, 255) d;\r\n  border-width: 10px;\r\n  border-style: solid;\r\n}\r\n\r\n.postInfo {\r\n  margin: 10%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  border-color: rgb(62, 54, 134) d;\r\n  border-width: 10px;\r\n  border-style: solid;\r\n  width: 100%;\r\n}\r\n\r\n.postInfo h2 {\r\n  font-size: 30px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}